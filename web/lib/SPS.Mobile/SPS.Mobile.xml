<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action name="GetSettings" />

 <group name="androidsetting">
    <action name="GetAndroidSettingList">
      <path>androidSettings/GetAndroidSettingListAction</path>
    </action>
    <action name="SaveAndroidSetting">
      <path>androidSettings/SaveAndroidSettingAction</path>
      <redirects>
        <redirect name="success" path="vt://androidSettings/" />
      </redirects>
    </action>
    <action name="DeleteAndroidSetting">
      <path>androidSettings/SaveAndroidSettingAction</path>
      <parameters>
        <request>
          <param name="action">'delete'</param>
        </request>
      </parameters>
    </action>
  </group>
