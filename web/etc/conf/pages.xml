                <page uri="appControls://(editor/)?comment-restore/">
                    <actions>SPS.Site.RestoreCommentAppControl</actions>
                </page>
                <page uri="appControls://comment-mark/">
                    <actions>SPS.Site.MarkCommentAppControl</actions>
                </page>
            </pageGroup>

            <pageGroup name="tools">
                <page uri="tools://init-editors/">
                    <actions>SPS.Common.InitEditorsTool</actions>
                </page>
            </pageGroup>

            <pageGroup name="im" boot="SPS.Site.GetSiteParams, SPS.VK.VKCheckAuth">
                <page uri="im://">
                    <template>tmpl://im/index.tmpl.php</template>
                </page>
                <page uri="im://login/" boot="">
                    <template>tmpl://im/login.tmpl.php</template>
                </page>
            </pageGroup>

            <pageGroup name="stat" boot="SPS.Site.GetSiteParams, SPS.VK.VKCheckAuth">
                <page uri="stat://">
                    <template>tmpl://st/index.tmpl.php</template>
                </page>
                <page uri="stat://reports/" boot="">
                    <template>tmpl://st/reports.tmpl.php</template>
                </page>
            </pageGroup>

            <pageGroup name="publicControls" boot="">
                <page uri="controls://upload-user-to-public/">
                    <actions>SPS.Site.UploadUserToPublic</actions>
                </page>
            </pageGroup>

            <pageGroup name="reports" boot="">
                <page uri="reports://topface-user-posts/">
                    <actions>SPS.Reports.TopfaceUserPosts</actions>
                </page>
            </pageGroup>

            <page uri="([^?]+)" boot="SPS.Site.GetSiteParams">
                <actions>SPS.Site.GetForceStaticPage</actions>
                <template>tmpl://fe/template.tmpl.php</template>
            </page>
        </pages>
    </site>
</sites>